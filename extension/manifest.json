{
  "name": "Game Cabinet Key Listener Extension",
  "description": "Listens for specific keys for FSU Art Game Cabinet",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "all_frames": true,
      "js": ["jquery-1.11.3.min.js", "contentscript.js"]
  }]

}
